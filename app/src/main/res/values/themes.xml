<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.BloomRoom" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textColor">@color/dark_violet</item>
        <item name="fontFamily">@font/dagestan</item>
        <!--Primary brand color -->
        <item name="colorPrimary">@color/purple</item>
        <item name="colorPrimaryVariant">@color/dark_violet</item>
        <item name="colorOnPrimary">@color/mediumPurple</item>

        <!--Secondary brand color -->
        <item name="colorSecondary">@color/light_pink</item>
        <item name="colorSecondaryVariant">@color/dark_violet</item>
        <item name="colorOnSecondary">@color/pink</item>
        <!--Status bar color-->
        <item name="android:statusBarColor" tools:targetApi="1">?attr/colorPrimary</item>

        <!-- Customize your light theme here. -->
    </style>

    <style name="Circular">
        <item name="cornerSize">50%</item>
    </style>
    <style name="Circular1">
        <item name="cornerSize">20%</item>
    </style>
    <style name="Theme.BloomRoom" parent="Base.Theme.BloomRoom" />

</resources>